<h1>Listing pnotes</h1>

<table>
  <tr>
    <th>Preport</th>
    <th>User</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pnotes.each do |pnote| %>
  <tr>
    <td><%= pnote.preport_id %></td>
    <td><%= pnote.user_id %></td>
    <td><%= pnote.content %></td>
    <td><%= link_to 'Show', pnote %></td>
    <td><%= link_to 'Edit', edit_pnote_path(pnote) %></td>
    <td><%= link_to 'Destroy', pnote, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Pnote', new_pnote_path %>
